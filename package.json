{
   "name": "@scrypted/nest",
   "version": "0.0.12",
   "description": "Scrypted plugin for Nest Thermostats and Cameras.",
   "author": "Scrypted",
   "license": "Apache",
   "scripts": {
      "prepublishOnly": "NODE_ENV=production scrypted-webpack",
      "prescrypted-vscode-launch": "scrypted-webpack",
      "scrypted-vscode-launch": "scrypted-deploy-debug",
      "scrypted-deploy-debug": "scrypted-deploy-debug",
      "scrypted-debug": "scrypted-debug",
      "scrypted-deploy": "scrypted-deploy",
      "scrypted-webpack": "scrypted-webpack"
   },
   "keywords": [
      "thermostat",
      "camera",
      "nest",
      "scrypted",
      "plugin"
   ],
   "scrypted": {
      "name": "Nest Controller",
      "type": "Device",
      "singleInstance": true,
      "interfaces": [
         "OauthClient",
         "DeviceProvider"
      ]
   },
   "devDependencies": {
      "@scrypted/sdk": "0.0.41"
   },
   "dependencies": {
      "axios": "^0.19.0",
      "eventsource": "^1.0.7",
      "lodash.invert": "^4.3.0",
      "query-string": "^6.4.0",
      "simple-oauth2": "^2.2.1"
   }
}
